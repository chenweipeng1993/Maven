<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>Ajax1Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax1Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax2Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax2Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax3Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax3Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax4Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax4Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax5Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax5Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax6Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax6Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ajax7Servlet</servlet-name>
    <servlet-class>cn.itcast.servlet.Ajax7Servlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>Ajax1Servlet</servlet-name>
    <url-pattern>/ajax1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax2Servlet</servlet-name>
    <url-pattern>/ajax2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax3Servlet</servlet-name>
    <url-pattern>/ajax3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax4Servlet</servlet-name>
    <url-pattern>/ajax4</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax5Servlet</servlet-name>
    <url-pattern>/ajax5</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax6Servlet</servlet-name>
    <url-pattern>/ajax6</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajax7Servlet</servlet-name>
    <url-pattern>/ajax7</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
